CC = gcc
CFLAGS = -Wall -Werror -Wextra -pedantic
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school

$(NAME) : $(SRC) m.h

shool.c : school.o

main.c : main.o


all: $(NAME)

fclean: oclean clean

clean:
	$(RM) $(NAME) *~ *.swp

oclean:
	$(RM) *.o

re: fclean all

.PHONY: all fclean clean re oclean
